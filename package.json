    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "highlight.js": "^11.8.0",
    "rehype-highlight": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.0",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.0.0",
    "unified": "^11.0.3"
