{
  "name": "@plumbiu/md",
  "version": "0.4.3",
  "description": "tools for markdown parser",
  "main": "dist/index.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Plumbiu/md"
  },
  "scripts": {
    "build": "tsup --config tsup.config.ts",
    "test": "vitest"
  },
  "files": [
    "dist",
    "style"
  ],
  "keywords": [
    "markdown",
    "md",
    "parse",
    "toc",
    "html",
    "blog"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./style/*": "./style/*"
  },
  "author": "Plumbiu",
  "license": "MIT",
  "devDependencies": {
    "@swc/core": "^1.3.95",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "html2toc": "^0.0.3",
    "rehype-highlight": "^7.0.0",
    "rehype-preset-minify": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.0",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.0.0",
    "simple-md-front-matter": "^0.0.4",
    "unified": "^11.0.3"
  }
}
